        e_aig: e_aig {
            label = "E_AIG";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press $kp LS(LC(U))>
                , <&macro_tap &kp KP_N0 &kp KP_N0 &kp E &kp KP_N9>
                , <&macro_release &kp LS(LCTRL)>
